{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wails: Debug myproject",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/DuDe",
      "preLaunchTask": "wails-debug",
      "cwd": "${workspaceFolder}",
      "env": {}
    },
    {
      "name": "Wails: Debug (Windows)",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/DuDe.exe",
      "preLaunchTask": "wails-debug-windows",
      "cwd": "${workspaceFolder}",
      "env": {}
    }
  ]
}